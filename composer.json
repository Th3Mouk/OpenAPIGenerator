{
    "name": "th3mouk/openapi-generator",
    "description": "PHP library which provide a scaffolding and generate an OpenAPI file",
    "license": "MIT",
    "authors": [
      {
        "name": "Jérémy Marodon",
        "email": "marodon.jeremy@gmail.com"
      }
    ],
    "require": {
      "php": ">=7.4",
      "ext-json": "*",
      "symfony/yaml": "^4.1|^5.0",
      "symfony/console": "^4.1|^5.0",
      "symfony/finder": "^4.1|^5.0"
    },
    "require-dev": {
      "phpstan/phpstan": "^0.12.33",
      "youdot/coding-standard": "^3.0"
    },
    "bin": ["openapi"],
    "autoload": {
      "psr-4": {
        "Th3Mouk\\OpenAPIGenerator\\": ""
      },
      "files": [
        "functions.php"
      ]
    }
}
